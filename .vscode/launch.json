{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "program": "${workspaceFolder}/node_modules/@teambit/bit/dist/app.js",
      "args": ["test"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/node_modules/@xian/**/*.js"
