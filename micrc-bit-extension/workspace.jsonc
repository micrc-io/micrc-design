{
  "$schema": "https://static.bit.dev/teambit/schemas/schema.json",
  "teambit.workspace/workspace": {
    "name": "micrc-bit-extension",
    "icon": "https://static.bit.dev/bit-logo.svg",
    "defaultDirectory": "{scope}/{name}",
    "defaultScope": "micrc.bit"
  },
  "teambit.dependencies/dependency-resolver": {
    "packageManager": "teambit.dependencies/pnpm",
    "policy": {
      "dependencies": {
        "@babel/runtime": "7.21.0",
        "@teambit/eslint-config-bit-react": "0.0.367",
        "@teambit/harmony": "0.4.6",
        "@types/jest": "29.4.0",
        "@types/node": "18.14.6",
        "@typescript-eslint/eslint-plugin": "5.54.0",
        "babel-plugin-transform-taroapi": "3.6.2",
        "chalk": "4.1.2",
        "copy-webpack-plugin": "11.0.0",
        "core-js": "3.29.0",
        "css-loader": "6.7.3",
        "eslint": "8.35.0",
        "eslint-import-resolver-node": "0.3.7",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-mdx": "2.0.5",
        "eslint-plugin-react": "7.32.2",
        "fast-json-patch": "3.1.1",
        "handlebars": "4.7.7",
        "lodash.omit": "4.5.0",
        "lodash.pick": "4.4.0",
        "loglevel": "1.8.1",
        "postcss": "8.4.21",
        "postcss-loader": "7.0.2",
        "postcss-pxtorem": "6.0.0",
        "postcss-pxtransform": "3.6.2",
        "prettier": "2.8.4",
        "resolve-url-loader": "5.0.0",
        "sass": "1.58.3",
        "sass-loader": "13.2.0",
        "stream-http": "3.2.0",
        "style-loader": "3.3.1",
        "typescript": "4.9.5",
        "url": "0.11.0",
        "util": "0.12.5",
        "webpack": "5.75.0",
        "zustand": "4.3.6"
      },
      "peerDependencies": {
        "@tarojs/components": "3.4.12",
        "@tarojs/taro": "3.4.12",
        "@tarojs/taro-h5": "3.4.12",
        "@testing-library/react": "12.1.5",
        "@testing-library/react-native": "11.5.2",
        "@types/lodash.omit": "4.5.7",
        "@types/lodash.pick": "4.4.7",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.71.3"
      }
    },
    "linkCoreAspects": true,
    "strictPeerDependencies": true
  },
  "teambit.workspace/variants": {
    "{compilations/**}": {
      "teambit.harmony/aspect": {}
    },
    "{generators/**}": {
      "teambit.harmony/aspect": {}
    },
    "{envs/**}": {
      "teambit.envs/env": {}
    },
    "{runtimes/**}": {
      "teambit.react/react": {}
    },
    "{_examples/web/components/**}": {
      "micrc.bit/envs/micrc-web": {}
    },
    "{_examples/web/modules/**}": {
      "micrc.bit/envs/micrc-web": {}
    },
    "{_examples/web/atoms/**}": {
      "micrc.bit/envs/micrc-web": {}
    },
    "{_examples/web/clientend/**}": {
      "micrc.bit/envs/micrc-web": {}
    },
    "{_examples/mini/components/**}": {
      "micrc.bit/envs/micrc-mini": {}
    },
    "{_examples/mini/modules/**}": {
      "micrc.bit/envs/micrc-mini": {}
    },
    "{_examples/app/components/**}": {
      "micrc.bit/envs/micrc-app": {}
    },
    "{_examples/app/modules/**}": {
      "micrc.bit/envs/micrc-app": {}
    }
  }
}